{
  "name": "yapi2str",
  "description": "将YAPI数据，格式化为字符串",
  "version": "1.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "/images/data.png",
    "32": "/images/data.png",
    "48": "/images/data.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "16": "/images/data.png",
    "32": "/images/data.png",
    "48": "/images/data.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/content.js"]
    }
  ]
}